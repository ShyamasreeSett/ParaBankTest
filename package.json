{
  "name": "sett-qa-suite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:ci": "npx playwright test --reporter=list",
    "test:e2e": "npx playwright test --grep \"@e2e\"",
    "test:main-suite": "npx playwright test --grep-invert \"@e2e\"",
    "test:allure": "npx playwright test && npx allure generate ./allure-results --clean -o ./allure-report && npx allure open ./allure-report",
    "teste2e:allure": "npx playwright test --grep \"@e2e\" && npx allure generate ./allure-results --clean -o ./allure-report && npx allure open ./allure-report"
  },
  "keywords": [],
