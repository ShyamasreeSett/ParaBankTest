  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.1",
    "allure-commandline": "^2.34.1",
    "allure-js-commons": "^3.4.1",
    "allure-playwright": "^3.4.1",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0"
  }
